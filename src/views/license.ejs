<h2>Skins installed</h2>
<ul>
    <% global.skins.forEach(e =>{ %>
        <li>
            <a href="<%= e.manifest.homepage %>"><%= e.manifest.name %></a> <small>v<%= e.manifest.version %></small>: <%= e.manifest.description %>
            <br>
            <%= e.manifest.author %>
            <br>
            <%= e.manifest.licence %>
        </li>
    <% }) %>
</ul>

<h2>Extensions Installed</h2>
<ul>
    <% for (e in global.extensions) { %>
        <li>
            <a href="<%= global.extensions[e].manifest.homepage %>"><%= global.extensions[e].manifest.name %></a> <small>v<%= global.extensions[e].manifest.version %></small>: <%= global.extensions[e].manifest.description %>
            <br>
            <%= global.extensions[e].manifest.author %>
            <br>
            <%= global.extensions[e].manifest.licence %>
        </li>
    <% } %>
</ul>

